{
  "@context": {
    "@version": 1.1,
    "@vocab": "https://genaptitude.io/ontology/uml/flow#",
    "uml": "http://www.omg.org/spec/UML/20180901/uml.xmi#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "Scenario": "uml:Scenario",
    "Step": "uml:SequenceNode",
    "Fragment": "uml:CombinedFragment",
    "scenarios": {
      "@id": "uml:hasScenario",
      "@type": "@id",
      "@container": "@set"
    },
    "steps": {
      "@id": "uml:hasStep",
      "@container": "@list"
    },
    "sequence": {
      "@id": "uml:sequenceOrder",
      "@type": "xsd:integer"
    },
    "condition": {
      "@id": "uml:hasCondition",
      "@type": "xsd:string"
    },
    "guard": {
      "@id": "uml:guardCondition",
      "@type": "xsd:string"
    },
    "source": {
      "@id": "uml:sequenceSource",
      "@type": "@id"
    },
    "target": {
      "@id": "uml:sequenceTarget",
      "@type": "@id"
    },
    "references": {
      "@id": "uml:referencesElement",
      "@type": "@id",
      "@container": "@set"
    }
  }
}
